    "zod",
    "typescript",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "oxlint": "^1.24.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "tsx": "^4.19.2",
    "zod": "^4.1.12"
  }
}
