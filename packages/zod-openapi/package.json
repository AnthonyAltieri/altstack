{
  "name": "@alt-stack/zod-openapi",
  "version": "1.0.1",
  "description": "Convert OpenAPI schemas to Zod schemas with TypeScript code generation",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "zod-openapi": "./bin/zod-openapi.js"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
